{
    "manifest_version": 3,
    "name": "Chrome Dashboard Extension",
    "version": "1.0.0",
    "description": "A simple dashboard Chrome extension",
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "background": {
    "service_worker": "background/background.js"
  },
    "permissions": ["bookmarks", "history", "storage"],
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "web_accessible_resources": [
    {
      "resources": ["icons/*"],
      "matches": ["<all_urls>"]
    }
  ]

  }

  